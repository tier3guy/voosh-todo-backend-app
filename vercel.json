{
    "version": 2,
    "builds": [
        {
            "src": "./dist/index.js",
            "use": "@vercel/node",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/src/index.js"
        }
    ]
}
